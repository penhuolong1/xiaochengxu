@import "colorui/main.wxss";
@import "colorui/icon.wxss";

/**app.wxss**/
page {
  width: 100%;
  height: 100%;
  position: relative;
  background-color: #ffffff;
}

@font-face {
  font-family: 'iconfont';
  /* project id 1168975 */
  src: url('//at.alicdn.com/t/font_1168975_uhg3jmqpzti.eot');
  src: url('//at.alicdn.com/t/font_1168975_uhg3jmqpzti.eot?#iefix') format('embedded-opentype'),
    url('//at.alicdn.com/t/font_1168975_uhg3jmqpzti.woff2') format('woff2'),
    url('//at.alicdn.com/t/font_1168975_uhg3jmqpzti.woff') format('woff'),
    url('//at.alicdn.com/t/font_1168975_uhg3jmqpzti.ttf') format('truetype'),
    url('//at.alicdn.com/t/font_1168975_uhg3jmqpzti.svg#iconfont') format('svg');
}

/* 自定义导航栏开始 */
.custom {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 45px;
  /* background: #c00; */
  z-index: 999;
  background: #fff;
  display: flex;
}

.textTit {
  width: 66%;
  opacity: 1;
  font-size: 18px;
  font-family: PingFang SC;
  font-weight: 600;
  color: rgba(34, 52, 94, 1);
  line-height: 45px;
  padding-left: 20px;
}

.textTit text {
  display: inline-block;
  max-width: 80%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.searchBarBox {
  width: 100%;
  height: 34px;
  background-color: #F2F5F8;
  border-radius: 10px;
  line-height: 34;
  margin-top: 7px;
}

.searchBarBox view {
  position: relative;
  display: inline-block;
  width: 80%;
}

.searchBarBox input {
  text-align: left;
  position: absolute;
  left: 14px;
  top: -309px;
  font-weight: normal;
  font-size: 15px;
  line-height: 34px;
}

.searchBarBox image {
  width: 20px;
  height: 20px;
  display: inline-block;
  float: right;
  margin: 7px 6px;
}

.textIco {
  width: 34px;
  height: 34px;
  margin-top: 7px;
  float: right;
  background-color: #F2F5F8;
  border-radius: 10px;
  line-height: 40px;
}

.textIco image {
  width: 20px;
  height: 20px;
  margin: 0 6px;
}

.empty_custom {
  box-sizing: content-box;
  height: 45px;
  width: 100%;
}

/* 自定义导航栏结束 */
/*隐藏滚动条*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 50rpx;
  color: #A8A8A8;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  display: block;
}

