/* pages/witness/evidenceList/evidenceList.wxss */
.content{
  height: calc(100% - 103px);
}
.right-cel{
  position: absolute;
  text-align: right;
  padding-right: 20px;
  right: 0px;
  top: 14px;
  color: #40A9FF;
}
.btns{
    
    color: rgb(255, 255, 255);
    width: 30%;
    margin-top: 0.2rem;
    display: inline-block;
    margin-right: 3%;
    border-radius: 200px;
    font-size: 14px;
}
.red{
  background-color: rgb(239, 79, 79);
}
.blue{
  background-color: rgb(64, 169, 255);
}
.green{
  background-color: rgb(4, 190, 2);
}
.picShow{
  position: absolute;
  width: 70%;
  height: 300px;
  top:50%;
  left:50%;
  margin-top:-220px;
  margin-left:-45%;
  /* padding:10px; */
  padding-top:40px;
  padding-left:40px;
  padding-right:40px;
  padding-bottom:40px;
  z-index: 99999999;
  background: #fff;
}
.picShow image{
  width: 100%;
  /* height:200px; */
}
.quit-b{
    background-color: white;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    position: absolute;
    right: 0px;
    top: 0px;
    cursor: pointer;
}
.quit {
    width: 40px;
    height: 40px;
}
.witness-footer{
  position:fixed;
  bottom:0px;
  width:100%;
  right:0px;
  left:0px;
  height: 60px;
}