.wrapper {
  width: 100%;
  height: 100%;
  background-color: #fff;
  margin-top: -20px;
  position: fixed;
  top: 150px;
}
.wrapper .law-detail-menu {
  height: 35px;
  line-height: 35px;
  display: flex;
  box-shadow:0px 3px 6px rgba(0,0,0,0.16);
}
.law-detail-menu .menu-item {
  flex: 1;
  text-align: center;
  color: #657496;
  font-size: 14px;
  font-weight: 600;
}
.law-detail-menu .menu-item.active text {
  color: #4285F4;
  border-bottom: 2px solid #4285F4;
  padding-bottom: 8px;
}
.content {
  padding: 20px;
  position: fixed;
  top: 160px;
  bottom: 0px;
}
.content .title-wrapper {
  font-size: 16px;
  font-weight: bolder;
}
.content .key-word-wrapper {
  margin-top: 10px;
  margin-bottom: 10px;
}
.content .key-word-wrapper .key-word {
  color: #186BFE;
  font-size: 12px;
  display: inline-block;
  margin-right: 10px;
  margin-bottom: 5px;
}
.content-box {
  border:1px solid rgba(242,245,248,1);
  padding: 0 12px;
  padding-bottom: 20px;
  margin-bottom: 10px;
}
.content-box .title {
  display: flex;
  height: 60px;
  line-height: 60px;
}
.content-box .title image{
  width: 13px;
  height: 10px;
  margin-right: 20px;
}
.content-box .title .title-text {
  color: #092058;
  font-size: 14px;
}
.content-box .data-wrapper {
  display: flex;
  justify-content: space-between;
  margin-bottom: 15px;
}
.content-box .data-wrapper .label {
  color: #4F6C8D;
  font-size: 12px;
}
.content-box .data-wrapper .text {
  color: #092058;
  font-size: 12px;
  max-width: 200px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}