/* pages/mediationRecord/mediationRecord.wxss */
.textIco image {
  width: 10px;
  height: 16px;
}

.textIco {
  text-align: center;
}

.empty-custom {
  height: 45px;
  width: 100%;
}

.wrapper {
  padding: 20px;
}

.wrapper .header {
  background-color: #F7FBFF;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #4285F4;
  border-radius: 15px;
  font-weight: bold;
}

.wrapper .title {
  display: flex;
  justify-content: space-between;
  margin-top: 20px;
  padding-bottom: 20px;
  border-bottom: 1px solid #F2F5F8;
}

.wrapper .title>text {
  font-size: 14px;
  color: #323643;
}

.wrapper .title .down-wrapper text {
  font-size: 12px;
  color: #727E8B;
  margin-right: 10px;
}

.wrapper .down-wrapper image {
  width: 15px;
  height: 8px;
}

.wrapper .sumbit-button {
  position: fixed;
  bottom: 20px;
  left: 20px;
  right: 20px;
  line-height: 50px;
  height: 50px;
  text-align: center;
  color: #fff;
  font-size: 14px;
  font-weight: bold;
  border-radius: 15px;
  background-color: #4285F4;
}
