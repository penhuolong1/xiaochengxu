.footer {
  position: absolute;
  bottom: 30px;
  left: 0;
  right: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  vertical-align: middle;
}
 .item {
  margin: 4px;
}
button.plain {
  margin-top: 60%;
  color: #FAFAFA;
  background-color: #34AADC;
  line-height: 50px;
  height: 50px;
  border-radius: 100px;
}
button.plain.button-hover {
  color: #FAFAFA;
  background-color: rgba(52, 170, 220, 0.808);
}
.logo {
  width: 48px;
  height: 48px;
}
.app-icon {
  border-radius: 50%;
}
.main{
  padding-right:20px;
  padding-left:20px;
}
.headers{
  margin-top: 25px
}

