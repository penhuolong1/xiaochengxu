.listcontent{
  overflow-y: scroll;
  height: calc(100% - 75px);
  margin-top: 20px;
}
.footer-item {
  width: 50%;
}
.menu-footer{
  /* top: 75px; */
  position: relative;
}
.textIco-b{
  float: left;
  text-align: center;
  margin-right: 5px;
}
.textIco-b image{
  width: 12px;
}
.filters-mask{
  width: 100%;
  height: 100%;
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9999;
}

.listMain{
  height:calc(100% - 135px);
  position: relative;
}


.caseList {
  width: 90%;
  display: flex;
  margin: 0 auto;
  line-height: 60px;
  height: 60px;
  border-bottom: 1px solid #F2F5F8;
}
.caseList .left { 
  width:100px;
  font-size: 14px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.caseList .right{
  flex-grow: 1;
  max-width: calc(100% - 100px);
  color: #727E8B;
  font-size: 13px;
}
.caseList .right text{
  display: inline-block;
  width: 100%;
  text-align: right;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.submit-add{
  width: 47%;
  height: 88rpx;
  line-height: 88rpx;
  border-radius: 44rpx;
  background: #4285F4;
  color: #fff;
  font-size: 34rpx;
  float: right;
}
.submit-cancel{
  width: 47%;
  height: 88rpx;
  line-height: 88rpx;
  border-radius: 44rpx;
  background: #fff;
  color: rgba(27,45,93,1);
  font-size: 34rpx;
  float: left;
  border: 1px solid rgba(226,230,236,1);
}