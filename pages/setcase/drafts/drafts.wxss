/* page{
  height: calc(100% - 120px)
} */
.listcontent{
  overflow-y: scroll;
  height: calc(100% - 75px);
  margin-top:10px;
}
.caseList {
  width: 90%;
  display: flex;
  margin: 0 auto;
  line-height: 60px;
  border-bottom: 1px solid #F2F5F8;
}
.caseList .left { 
  flex-grow: 1;
  font-size: 14px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.caseList .right{
  width:100px;
  line-height:52px;
}
.caseList .right .check {
  color: #86D530;
  float: right;
}
.footer-item {
  width: 50%;
}
.menu-footer{
  /* top: 75px; */
  position: relative;
}
.textIco-b{
  float: left;
  text-align: center;
  margin-right: 5px;
}
.textIco-b image{
  width: 12px;
}
.filters-mask{
  width: 100%;
  height: 100%;
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9999;
}


.light{
    display: inline-block;
    width: 25px;
    height: 25px;
    border-radius: 50%;
    position: absolute;
    right:15px;
    line-height: 57px;
}
.open{
    background: #04d712;
    box-shadow:  1px 2px 6px #5ff75f;
}
.disable{
  background: #999;
  box-shadow: inset 0px 0px 10px #eee;
}
.listMain{
  height:calc(100% - 135px);
  position: relative;
}

.loads{
  color: #fff
}
.i-load-more-tip{
  color: #fff !important;
}

