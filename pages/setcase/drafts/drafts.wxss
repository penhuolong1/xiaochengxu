.listLine{
  display:flex;
  height: 60px;
  line-height: 60px;
  width: 90%;
  margin: 0 auto;
  border-bottom:1px solid  rgba(242,245,248,1.00);
}
.listLine .left{
  order:1;
  width:100px;
  color:#323643;
  padding-left:10px;
  font-size:15px;
}
.listLine .right{
  order:3;
  width:30px;
  text-align: right;
  font-weight: bold;
  font-size:14px;
}
.listLine .main{
  order:2;
  flex-grow:1;
