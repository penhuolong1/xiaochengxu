/* pages/add-applicant/applicantForm/applicantForm.wxss */
.wrapper {
  padding: 10px 20px;
}

.form-item {
  display: flex;
  line-height: 40px;
  justify-content: space-between;
  padding: 7px 20px;
  border-bottom: 1px solid #F2F5F8;
  margin-left: 20px;
  padding-left: 0px;
}

.listcontent {
  position: fixed;
  bottom: 110px;
  top: 110px;
  left: 0px;
  right: 0px;
  padding: 0 20px;
}

.minTit {
  line-height: 50px;
  color: #495060;
  font-size: 14px;
  margin-left: 20px;
}

.upload {
  margin: 10px auto 0 auto;
  border: 1px solid #F2F5F8;
}

.uploadBox {
  width: 49%;
  display: inline-block;
  text-align: center;
  height: 200px;
  line-height: 35px;
  color: #727E8B;
  font-size: 12px;
}

.uploadBox image {
  width: 80%;
  margin: 40px auto 0 auto;
  height: 104px;
}

.btn-wrapper {
  margin-top: 10px;
  position: fixed;
  left: 0px;
  right: 0px;
  padding: 0 20px;
  bottom: 10px;
  background: #fff;
}

.btn-wrapper .btn1 {
  text-align: center;
  border: 1px solid #E2E6EC;
  height: 50px;
  line-height: 50px;
  border-radius: 25px;
  color: #1B2D5D;
  font-size: 14px;
}

.btn-wrapper .btn2 {
  text-align: center;
  height: 50px;
  line-height: 50px;
  background-color: #4285F4;
  border-radius: 25px;
  color: #fff;
  font-size: 14px;
  margin-top: 5px;
}

.uploadIco {
  width: 50px;
  height: 50px;
  color: #fff;
  background-color: #4285F4;
  border-radius: 50%;
  text-align: center;
  margin: 50px auto 0 auto;
  line-height: 45px;
}

/* 代理人权限部分 */
.author-wrapper {
  position: fixed;
  bottom: 0px;
  left: 0px;
  top: 0px;
  right: 0px;
  background-color: white;
  z-index: 10000;
  padding: 0 20px;
}

.author-wrapper .title {
  height: 40px;
  line-height: 40px;
  text-align: center;
  color: #4285F4;
  font-size: 14px;
  border-radius: 20px;
  font-weight: bold;
  background-color: #F7FBFF;
  margin-top: 10px;
}

.author-wrapper .content {
  position: absolute;
  top: 50px;
  left: 0px;
  right: 0px;
  bottom: 80px;
  padding: 0 20px;
}

.author-wrapper .item {
  display: flex;
  height: 60px;
  align-items: center;
  border-bottom: 1px solid #F2F5F8;
}

.author-wrapper .item .icon {
  width: 35px;
}

.author-wrapper .item .icon image {
  width: 15px;
  height: 12px;
  margin-right: 10px;
  margin-left: 10px;
}

.author-wrapper .item .text {
  font-size: 14px;
  color: #323643;
  line-height: 22px;
  flex: 1;
}

.submit-btn {
  text-align: center;
  height: 50px;
  line-height: 50px;
  background-color: #4285F4;
  border-radius: 25px;
  color: #fff;
  font-size: 14px;
  margin-top: 5px;
  position: fixed;
  bottom: 20px;
  left: 20px;
  right: 20px;
}

.explain-wrapper {
  border: 1px solid #F2F5F8;
  padding-bottom: 10px;
}

.explain-wrapper .label {
  height: 60px;
  display: flex;
  align-items: center;
}

.explain-wrapper .label .icon {
  width: 35px;
}

.explain-wrapper .label .icon image {
  width: 15px;
  height: 12px;
  margin-right: 10px;
  margin-left: 10px;
}

.explain-wrapper .text-content {
  padding: 0 20px;
}

.lawyer-wrapper {
  padding: 0 20px;
}

.lawyer-wrapper .lawyer-item {
  display: flex;
  height: 60px;
  justify-content: center;
  align-items: center;
  border-bottom: 1px solid #F2F5F8;
}

.lawyer-wrapper .lawyer-item .text {
  flex: 1;
  color: #323643;
  font-size: 14px;
}

.lawyer-wrapper .lawyer-item .icon {
  width: 50px;
  text-align: right;
}

.no-wrapper {
  position: fixed;
  top: 110px;
  background-color: #fff;
  bottom: 0px;
  width: 100%;
  z-index: 1000;
}

.author-item {
  display: flex;
  align-items: center;
  padding: 7px 20px;
  border-bottom: 1px solid #F2F5F8;
  margin-left: 20px;
  padding-left: 0px;
}

.author-item .icon {
  width: 20px;
}

.author-item .text {
  flex: 1;
  line-height: 20px;
}

.author-item .icon image {
  width: 12px;
  height: 10px;
}
